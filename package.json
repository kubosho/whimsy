{
    "name": "whimsy",
    "displayName": "Whimsy",
    "description": "Color theme randomly change for VS Code",
    "version": "0.1.0",
    "publisher": "kubosho",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.activate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.activate",
                "title": "Whimsy: activate"
            },
            {
                "command": "extension.updateInterval",
                "title": "Whimsy: update interval"
            },
            {
                "command": "extension.updateThemeList",
                "title": "Whimsy: update theme list"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/node": "^6.0.40",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "lodash.shuffle": "^4.2.0"
    }
}
